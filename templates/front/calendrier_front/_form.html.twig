{{ form_start(form) }}
    <div class="form-calendrier row">
        <div class="label-new col-sm-12 col-md-12 col-lg-6">
            {{ form_row(form.titre) }}
        </div>
    </div>
    <div class="form-calendrier row">
        <div class="label-new col-sm-12 col-md-6 col-lg-6">
            {{ form_row(form.start_date) }}
        </div>
        <div class="form-error">
            {{ form_errors(form.start_date) }}
        </div>
        <div class="label-new col-sm-12 col-md-6 col-lg-6">
            {{ form_row(form.end_date) }}
        </div>
        <div class="form-error">
            {{ form_errors(form.end_date) }}
        </div>
    </div>
    <div class="form-calendrier row">
        <div class="label-new col-sm-12 col-md-6 col-lg-6">
            {{ form_row(form.coach) }}
        </div>
        <div class="label-new col-sm-12 col-md-6 col-lg-6">
            {{ form_row(form.sportif) }}
        </div>
    </div>
    <div class="form-group">
        <button class="btn-edit btn btn-primary" type="submit">
            Envoyer
        </button>
    </div>
{{ form_end(form) }}


