{% extends 'base.html.twig' %}

{% block title %}Accueil{% endblock %}

{% block body %}
    <div class="intro-form" style="background-image: url('{{ asset('asset_front/img/coureur.jpg') }}')">
        <div class="container">
            <div class="col-md-12">
                <h2 translate="HP_INTRO" class="ng-scope">RÃ©serve ton coach sportif<br><small>Plus de 100 sports et 2000 coachs dans toute la France</small></h2>
                <h3></h3>
            </div>
            <div class="col-md-8 col-md-offset-2">
                <div class="row gutter-10" ng-show="!searchByName">
                    <form ng-submit="onSearch()" class="search-container ng-pristine ng-valid ng-isolate-scope" submit="onSearch(search)">
                        <div class="col-sm-9">
                            <div class="col-xs-6 mb-xs-16 input-container">
                                <div class="sport-select ng-isolate-scope" ng-mouseenter="isOverSportsList = true" ng-mouseleave="isOverSportsList = false" search="search">
                                    <input type="text" ng-model="search.sport" translate-once-placeholder="GLOBAL_SPORT" class="sport typeahead ng-pristine ng-untouched ng-valid ng-not-empty" ng-focus="sportAutocomplete()" ng-blur="hideSportAutocomplete()" ng-change="sportAutocomplete()" ng-enter="selectFirstSport()" placeholder="Sport">
                                    <ul ng-show="filteredSportList.length > 0" class="dropdown-search ng-hide">
                                        <!-- ngIf: userFavoritesSports.length > 0 -->
                                        <!-- ngRepeat: sport in userFavoritesSports -->
                                        <!-- ngIf: userFavoritesSports.length > 0 -->
                                        <!-- ngRepeat: sport in filteredSportList -->
                                    </ul>
                                </div>
                            </div>
                            <div class="col-xs-6 mb-xs-16 input-container" ng-mouseenter="isOverCityList = true" ng-mouseleave="isOverCityList = false">
                                <input type="text" ng-model="search.city" ng-model-options="{ debounce: 500 }" translate-once-placeholder="GLOBAL_CITY" class="col-xs-6 lieu ng-pristine ng-untouched ng-valid ng-not-empty" ng-change="cityAutocomplete()" ng-enter="selectFirstCity()" ng-focus="showCityDropdown()" ng-blur="hideCityAutocomplete()" placeholder="Ville">
                                <ul ng-show="showCityList" class="dropdown-search ng-hide">
                                    <li ng-show="search.city.length < 3" class="heading geoloc ng-scope ng-hide" ng-click="askGeolocation()" translate="SEARCH_YOURPOSITION">VOTRE POSITION</li>
                                    <li ng-show="search.city.length < 3" ng-click="askGeolocation()" class="geoloc-btn ng-hide"><strong translate="SEARCH_AROUND" class="ng-scope">Chercher dans les alentours</strong></li>
                                    <!-- ngIf: filteredCityList.length > 0 -->
                                    <!-- ngRepeat: city in filteredCityList -->
                                </ul>
                                <!-- ngIf: geolocSearching -->
                            </div>
                        </div>
                        <div class="col-sm-3 col-xs-12 search-button-container">
                            <input type="submit" class="bgy full" translate-once-value="GLOBAL_SEARCH_VERB" ng-disabled="!isValidSearch()" value="Rechercher">
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
