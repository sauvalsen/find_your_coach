      </div>
      <!-- /main content -->


      <!-- Opposite sidebar -->
        {{ include(
          '@AdminBundle/Menu/oposite_sideBar.html.twig'
        ) }}
        {# {{ include("@AdminBundle/Menu/oposite_sideBar.html.twig") }} #}
      <!-- /opposite sidebar -->

    </div>
    <!-- /page content -->
    {% endblock %}
  </div>
  <!-- /page container -->
  {% endblock %}

   <!-- block modal -->
  {% block modal %}
    
  {% endblock %}
