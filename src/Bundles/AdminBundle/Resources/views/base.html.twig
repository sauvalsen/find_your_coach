  {% block javascripts %}
    <!-- Core JS files -->
    <script type="text/javascript" src="{{ asset('bundles/admin/js/core/libraries/bootstrap.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admin/js/core/libraries/pickers/anytime.min.js') }}"></script>
