          <div class="breadcrumb-line">
            <ul class="breadcrumb">
              <li><a href="{{ path('admin_homepage') }}"><i class="icon-home2 position-left"></i>Home</a></li>
              {% block titre_menu %}{% endblock %}
              <li class="active">{% block nom_de_page %}{% endblock %}</li>
            </ul>

{#            <ul class="breadcrumb-elements">#}
{#              <li><a href="#"><i class="icon-comment-discussion position-left"></i> Support</a></li>#}
{#              <li class="dropdown">#}
{#                <a href="#" class="dropdown-toggle" data-toggle="dropdown">#}
{#                  <i class="icon-gear position-left"></i>#}
{#                  Settings#}
{#                  <span class="caret"></span>#}
{#                </a>#}
{#                <ul class="dropdown-menu dropdown-menu-right">#}
{#                  <li><a href="#"><i class="icon-user-lock"></i> Account security</a></li>#}
{#                  <li><a href="#"><i class="icon-statistics"></i> Analytics</a></li>#}
{#                  <li><a href="#"><i class="icon-accessibility"></i> Accessibility</a></li>#}
{#                  <li class="divider"></li>#}
{#                  <li><a href="#"><i class="icon-gear"></i> All settings</a></li>#}
{#                </ul>#}
{#              </li>#}
{#            </ul>#}
          </div>
        </div>
        <!-- /page header -->     

        <!-- Content area -->
        <div class="content">

          <!-- Info alert 
          <div class="alert alert-info alert-styled-left alert-arrow-left alert-component">
            <button type="button" class="close" data-dismiss="alert"><span>&times;</span><span class="sr-only">Close</span></button>
            <h6 class="alert-heading text-semibold">Double sidebar. Fix default sidebar width</h6>
            When the opposite sidebar is shown, main sidebar width stays the same. This works for mini sidebar type as well.
            </div>
            /info alert -->
          
          {% block content %}

