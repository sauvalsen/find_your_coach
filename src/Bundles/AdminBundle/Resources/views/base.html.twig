<!DOCTYPE html>
<html lang="fr">
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>{% block title %}Administration{% endblock %}</title>

  <!-- Global stylesheets -->
  {% block stylesheets %}
      <link href="https://fonts.googleapis.com/css?family=Roboto:400,300,100,500,700,900" rel="stylesheet" type="text/css">
      <link href="{{ asset('bundles/admin/fonts/icomoon/styles.css') }}" rel="stylesheet" type="text/css">
      <link href="{{ asset('bundles/admin/css/minified/bootstrap.min.css') }}" rel="stylesheet" type="text/css">
      <link href="{{ asset('bundles/admin/css/minified/core.min.css') }}" rel="stylesheet" type="text/css">
      <link href="{{ asset('bundles/admin/css/minified/components.min.css') }}" rel="stylesheet" type="text/css">
      <link href="{{ asset('bundles/admin/css/minified/colors.min.css') }}" rel="stylesheet" type="text/css">
      <link href="{{ asset('asset_back/css/style.css') }}" rel="stylesheet" type="text/css">
  {% endblock %}
  <!-- /global stylesheets -->
<!-- Core JS files -->
  <script type="text/javascript" src="{{ asset('bundles/admin/js/core/libraries/jquery.min.js') }}"></script>
</head>

{% block body %}
<body class="{% block body_class %}sidebar-secondary-hidden{% endblock %}">
   
  <!-- Main navbar -->
    {{ include(
        '@AdminBundle/Menu/main_navBar.html.twig'
    ) }}
    {# {{ include("@AdminBundle/Menu/main_navBar.html.twig") }} #}
  <!-- /main navbar -->

  {% block page_container %}
  <!-- Page container -->
  <div class="page-container">
    {% block page_content %}
